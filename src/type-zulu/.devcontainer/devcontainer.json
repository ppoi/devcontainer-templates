{
  "name": "type-zulu",
  "image": "ghcr.io/ppoi/devcontainer-images/base:latest",
  "containerUser": "vscode",
  "mounts": [{"source": "/var/run/docker.sock", "target": "/var/run/docker.sock", "type": "bind"}],
  "capAdd": ["AUDIT_WRITE", "NET_RAW"],
  "customizations": {
    "vscode": {
      "settings": {
        "files.encoding": "utf8",
        "files.eol": "\r\n",
        "editor.useTabStops": false
      }
    }
  }
}