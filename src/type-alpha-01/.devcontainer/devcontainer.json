{
  "name": "type-alpha",
  "image": "ghcr.io/ppoi/devcontainer-images/type-alpha:latest",
  "mounts": [{"source": "/var/run/docker.sock", "target": "/var/run/docker.sock", "type": "bind"}],
  "capAdd": ["AUDIT_WRITE", "NET_RAW"],
  "customizations": {
    "vscode": {
      "extensions": [
        "svelte.svelte-vscode",
        "redhat.vscode-yaml"
      ],
      "settings": {
        "files.encoding": "utf8",
        "files.eol": "\r\n"
      }
    }
  }
}